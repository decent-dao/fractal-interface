    "@fontsource/ibm-plex-sans": "^4.5.13",
    "@fractal-framework/fractal-contracts": "^0.1.16",
    "@rainbow-me/rainbowkit": "^0.8.1",
    "@safe-global/safe-ethers-lib": "^1.9.2",
    "@safe-global/safe-service-client": "^1.5.2",
    "@sentry/react": "^7.42.0",
    "@sentry/tracing": "^7.42.0",
    "@ukstv/jazzicon-react": "^1.0.0",
    "@walletconnect/ethereum-provider": "^1.8.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.29.3",
    "ethers": "^5.7.2",
    "formik": "^2.2.9",
    "framer-motion": "^6.5.1",
    "i18next": "^21.9.2",
    "i18next-browser-languagedetector": "^6.1.5",
    "js-big-decimal": "^1.3.12",
    "next": "^13.2.4",
    "react": "latest",
    "react-dom": "latest",
    "react-i18next": "^11.18.6",
    "react-idle-timer": "^5.5.3",
    "react-image": "^4.0.3",
    "react-toastify": "^9.0.8",
    "wagmi": "^0.9.6",
    "yup": "^1"
  },
  "scripts": {
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "set:env": "GENERATE_SOURCEMAP=false NEXT_PUBLIC_GIT_HASH=`git rev-parse HEAD`",
    "dev": "npm run set:env & next dev",
    "start": "npm run set:env & next start",
    "start:https": "npm run set:env & HTTPS=true next start",
    "start:windows": "set \"GENERATE_SOURCEMAP=false\" && for /F \"delims=\" %I in ('git rev-parse HEAD') do set \"NEXT_PUBLIC_GIT_HASH=%I\" && next start",
    "build": "NEXT_PUBLIC_GIT_HASH=`git rev-parse HEAD` next build",
    "build:windows": "for /F \"delims=\" %I in ('git rev-parse HEAD') do set \"NEXT_PUBLIC_GIT_HASH=%I\" && next build",
    "test": "npm run set:env & next test",
    "prepare": "husky install",
    "tests": "playwright test"
  },
  "engines": {
    "node": "18.12.1",
    "npm": "8.19.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.29.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "encoding": "^0.1.13",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.2.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "playwright": "^1.29.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.0",
    "url": "^0.11.0"
  }
}